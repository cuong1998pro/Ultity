             xmlns:local="clr-namespace:GiaoDien"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="xamlResource\TriggerResource.xaml"></ResourceDictionary>
                <ResourceDictionary Source="xamlResource\BaseResource.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>